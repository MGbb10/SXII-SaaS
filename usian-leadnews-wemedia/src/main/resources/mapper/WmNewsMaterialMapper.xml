<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.usian.wemedia.mapper.WmNewsMaterialMapper">
    <insert id="saveRelationsBycontent">
        insert into
        we_news_material(material_id,news_id,type,ord)
        values
        <foreach collection="materials" item="mid" index="ord" separator=",">
            (#{mid},#{newsId},#{type},#{ord})
        </foreach>
    </insert>
</mapper>